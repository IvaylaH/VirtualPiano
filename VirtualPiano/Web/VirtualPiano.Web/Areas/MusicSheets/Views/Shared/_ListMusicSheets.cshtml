@using VirtualPiano.Web.Areas.MusicSheets.ViewModels
@model IEnumerable<CollectionOfSheetViewModel>

<div class="panel-success">
    <div class="panel-heading">
        <h2 class="panel-title songs-title">Songs List</h2>
    </div>
    <div class="panel-body">
        <ul class="songs-list">
            @foreach (var song in Model)
            {
                <li>
                    <h4>@Html.ActionLink(song.Title, "Details", "Sheets", new { id = song.Id }, null)</h4>
                </li>
            }
        </ul>
    </div>
</div>
